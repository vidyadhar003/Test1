package com.exceptionprograme;



public class ExceptionDemo {

	public static void main(String[] args) {
		
		
		Object obj=new Object();
		
		
		String str=null;
		
		
		
		try {

			Integer s = new Integer("abb");

			String a = args[1];
			
			System.out.println(a);
			
			System.out.println(str.length());
		
			ExceptionDemo em= (ExceptionDemo)obj;
			
			System.out.println(em);
			
			int i = 10 / 0;

			System.out.println(i);

			System.out.println(s);

		} catch (ArithmeticException | NumberFormatException |NullPointerException |ClassCastException e) {

			System.out.println("Array out of bound Exception is handled");

			System.out.println("Number format exception is handend");

			System.out.println("Divdie by zero exception is handeld");

			e.printStackTrace();
		}

	}

}
